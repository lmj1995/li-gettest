		TreeMap<Integer, Student> map = new TreeMap<Integer, Student>();
		
		Student s1 = new Student("aaa", 20);
		Student s2 = new Student("bbb", 21);
		Student s3 = new Student("ccc", 22);
		Student s4 = new Student("ddd", 23);
		
		map.put(4, s1);
		map.put(1, s2);
		map.put(3, s3);
		map.put(2, s4);
		map.put(5, s4);
		System.out.println(map);
		
		
		map.keySet();
		map.values();
		map.entrySet();
		
		
