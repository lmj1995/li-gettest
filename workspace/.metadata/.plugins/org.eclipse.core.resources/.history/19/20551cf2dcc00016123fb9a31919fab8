package com.lmj.test;
/**
 *实现runable接口 
 *
 */
public class Dome3 implements  Runnable {

	@Override
	public void run() {

		int count = 100;
		int i = 0;
		while (i<count) {
//           获取运行这段代码的运行名字
			String name = Thread.currentThread().getName();
			System.out.println(name+";i="+i);
			i++;
		}
		
		
	}
	

}
